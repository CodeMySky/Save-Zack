<div class="denglu-board hide">
	<style>
		div.denglu-board {
			z-index: 5;
			position: fixed;
			width: 100%;
			height: 100%;
			background-color: rgba(0, 0, 0, 0.3);
			top: 0;
			left: 0;
		}
		div.denglu-box{
			width: 452px;
			margin: auto;
			display: block;
			background: white;
			margin-top: 60px;
			border-radius: 5px;
			padding: 15px;
			border: 5px solid gray;
		}
		div.denglu-box>a {
			height: 90px;
			display: inline-block;
			width: 91px;
			height: 35px;
		}
		div.denglu-box>a.sina {
			background-image: url(/images/Sina_1.gif);
		}
		div.denglu-box>a.renren {
			background-image: url(/images/Renren_1.gif);
		}
		div.denglu-box>a.qq {
			background-image: url(/images/Qzone_1.gif);
		}
	</style>
	<div class="denglu-box">
		<button type="button" class="close" data-dismiss="denglu-board" aria-hidden="true">×</button>
		<h6>合作网站账号登陆</h6>
		<a class='sina' target="_blank" href="http://open.denglu.cc/transfer/sina?appid=50460&redirect_uri=http%3A%2F%2Fsavezack.cn%2F"></a>
		<a class='renren' target="_blank" href="http://open.denglu.cc/transfer/renren?appid=50460&redirect_uri=http%3A%2F%2Fsavezack.cn%2F"></a>
		<a class='qq' target="_blank" href="http://open.denglu.cc/transfer/qzone?appid=50460&redirect_uri=http%3A%2F%2Fsavezack.cn%2F"></a>
	</div>
	<script>
	mc2.denglu = function () {
		$(function () {
			$(".profile:has(i)").click( function (e) {
				$(".denglu-board").removeClass("hide");
				e.preventDefault();
			});
		});
	};
	</script>
</div>

